order-radius: 4px;\r\n  background: white;\r\n\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: flex-start;\r\n\r\n  padding: 39px 29px 29px 29px;\r\n  width: 320px;\r\n\r\n  .first_line,\r\n  .right_h,\r\n  .left_h{\r\n    @extend %stroke;\r\n\r\n  }\r\n  .price{\r\n    margin: 18px 0 0 1px;\r\n    p{margin: 0;}\r\n    @extend %stroke;\r\n    @extend %three_price;\r\n  }\r\n  .service{\r\n    margin: 10px 0 0 1px;\r\n    p{margin: 0;}\r\n    @extend %stroke;\r\n    @extend %three_price;\r\n  }\r\n  .add_service{\r\n    line-height: 18px;\r\n    margin: 14px 0 0 1px;\r\n    p{margin: 0;}\r\n    @extend %stroke;\r\n    @extend %three_price;\r\n\r\n  }\r\n  .total_price{\r\n    margin: 10px 0 0 1px;\r\n    @include Font_QuickSand(19px, bold, 24px, #1F2041);\r\n    font-family: Open-Sans, sans-serif;\r\n    font-style: normal;\r\n    font-weight: bold;\r\n    font-size: 19px;\r\n    line-height: 24px;\r\n    /* identical to box height */\r\n\r\n\r\n    /* Dark 100% */\r\n\r\n    color: #1F2041;\r\n    @extend %stroke;\r\n  }\r\n  .room_status{\r\n    @include Font_Montserrat-Bold(12px,  17px, #BC9CFF);\r\n    margin-left: 5px;\r\n    text-transform: uppercase;\r\n  }\r\n\r\n  .room__number_room{\r\n    @include Font_QuickSand(24px,bold,30px,#1F2041);\r\n    margin: 0 0 16px 0;\r\n  }\r\n\r\n  .room_price{\r\n    @include Font_Montserrat-Bold(14px,18px,rgba(31, 32, 65, 0.5));\r\n    margin-top: 10px;\r\n  }\r\n\r\n  .room_price_per_day{\r\n    @include Font_Montserrat-Regular(12px,18px,rgba(31, 32, 65, 0.5));\r\n    margin-top: 10px;\r\n  }\r\n\r\n  .tell_n{\r\n    @include Font_QuickSand(14px,900,17px,#1F2041);\r\n    margin-top: 10px;\r\n  }\r\n\r\n  .arr_lea_gue{\r\n\r\n  }\r\n\r\n  .first_line{\r\n    p{}\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"7dd0429e26adeb58983d87c1c57311af","cacheData":{"env":{}}}