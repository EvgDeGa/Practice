x;\r\n  border-radius: 4px;\r\n\r\n  outline: none;\r\n\r\n}\r\n\r\n@mixin Font_Montserrat-Regular($size, $height, $color){\r\n  font-family: Montserrat-Regular, sans-serif;\r\n  font-style: normal;\r\n  font-weight: 400;\r\n  font-size: $size;\r\n  line-height: $height;\r\n  color: $color;\r\n}\r\n\r\n@mixin Font_Montserrat-Bold($size, $height, $color){\r\n  font-family: Montserrat-Bold, sans-serif;\r\n  font-style: normal;\r\n  font-weight: 700;\r\n  font-size: $size;\r\n  line-height: $height;\r\n  color: $color;\r\n\r\n}\r\n\r\n@mixin Font_QuickSand($size, $weight, $height, $color){\r\n  font-family: Quicksand-Regular, sans-serif;\r\n  font-style: normal;\r\n  font-weight: $weight;\r\n  font-size: $size;\r\n  line-height: $height;\r\n  color: $color;\r\n  letter-spacing: 2px;\r\n}\r\n\r\n\r\n\r\n@mixin Font_Open-Sans($size, $weight, $height, $color){\r\n  font-family: Open-Sans, sans-serif;\r\n  font-style: normal;\r\n  font-weight: $weight;\r\n  font-size: $size;\r\n  line-height: $height;\r\n  color: $color;\r\n}\r\n\r\n\r\nli{\r\n  list-style-type: none;\r\n}\r\n\r\na{\r\n  text-decoration: none;\r\n}\r\n//@font-face {\r\n//  font-family: \"Open-Sans\";\r\n//  src: url(\"\") format(\"ttf\");\r\n//}\r\n\r\n@font-face {\r\n  font-family: \"Quicksand-Regular\";\r\n  src: url(\"../../font/Quicksand/Quicksand-Regular.woff\") format(\"woff\");\r\n}\r\n\r\n@font-face {\r\n  font-family: \"Montserrat-Regular\";\r\n  src: url(\"../../font/Montserrat/Montserrat-Regular.woff\") format(\"woff\");\r\n}\r\n\r\n@font-face {\r\n  font-family: \"Montserrat-Bold\";\r\n  src: url(\"../../font/Montserrat/Montserrat-Bold.woff\") format(\"woff\");\r\n}\r\n","blocks\\checkbox_button\\checkbox_button.scss":"@import \"src/mixin/mixin.scss\";\r\n\r\n.checkbox_button{\r\n  margin-top: 30px;\r\n  position: relative;\r\n  width: 266px;\r\n  display: flex;\r\n  flex-direction: column;\r\n\r\n  *{\r\n    user-select: none;\r\n  }\r\n\r\n\r\n\r\n  .checkbox {\r\n    padding: 5px 0 ;\r\n\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: flex-start;\r\n    height: 100%;\r\n\r\n\r\n  }\r\n\r\n  .container {\r\n\r\n    display: block;\r\n    position: relative;\r\n\r\n    padding-left: 30px;\r\n    margin-right: 20px;\r\n    cursor: pointer;\r\n\r\n    p {\r\n      display: block;\r\n      margin: 0;\r\n      @include Font_Montserrat-Regular(14px, 20px, rgba(31, 32, 65, 0.75));\r\n    }\r\n    input {\r\n      position: absolute;\r\n\r\n      opacity: 0;\r\n      cursor: pointer;\r\n    }\r\n  }\r\n\r\n  .checkmark {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    height: 18px;\r\n    width: 18px;\r\n    background: #FFFFFF;\r\n\r\n    border: 1px solid rgba(31, 32, 65, 0.25);\r\n    border-radius: 4px;\r\n  }\r\n\r\n  .container input:checked ~ .checkmark {\r\n    border: 1px solid #BC9CFF;\r\n    background: white;\r\n  }\r\n\r\n  .checkmark:after {\r\n    content: \"\";\r\n    position: absolute;\r\n    display: none;\r\n  }\r\n\r\n  .container input:checked ~ .checkmark:after {\r\n    display: block;\r\n  }\r\n\r\n  .container .checkmark:after {\r\n    top: 5px;\r\n    left: 5px;\r\n    width: 6px;\r\n    height: 4px;\r\n    border-bottom: 2px solid #BC9CFF;\r\n    border-left: 2px solid #BC9CFF;\r\n    transform: rotate(-45deg);\r\n\r\n  }\r\n\r\n  .checkbox_button_H{\r\n    margin: 0 0  11px 0;\r\n    @include Font_Montserrat-Bold(12px,15px,#1F2041);\r\n    text-transform: uppercase;\r\n  }\r\n\r\n  .checkbox:last-child{\r\n    padding-bottom: 0;\r\n  }\r\n\r\n}\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"7100cc7365001acac823442f41c82313","cacheData":{"env":{}}}